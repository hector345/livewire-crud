{
    "name": "hector345/livewire-crud-vuexy",
    "description": "Scaffold Livewire Componentes, migraciones, f√°brica y crud en un solo comando basado en tablas de base de datos",
    "keywords": [
        "hector345",
        "livewire-crud"
    ],
    "homepage": "https://github.com/hector345/livewire-crud",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "George Chitechi",
            "email": "george@flightsadmin.com",
            "role": "Developer"
        },
        {
            "name": "Hector",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0",
        "laravel/ui": "^2|^3",
        "livewire/livewire": "^2"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Hectoradmin\\LivewireCrud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hectoradmin\\LivewireCrud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hectoradmin\\LivewireCrud\\LivewireCrudServiceProvider"
            ],
            "aliases": {
                "LivewireCrud": "Hectoradmin\\LivewireCrud\\LivewireCrudFacade"
            }
        }
    }
}
